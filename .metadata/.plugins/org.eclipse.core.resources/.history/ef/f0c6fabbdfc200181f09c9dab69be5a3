package first;

public class Activity3 {

	public static void main(String[] args) {
		egg()

	}
	
	public static void openDown(String[] args) {
		System.out.println("  ______");
		System.out.println(" /      \\");
		System.out.println("/        \\);
	}
	
	public static void openUp(String[] args) {
		System.out.println("\\        /");
		System.out.println(" \\______/");
	}
	
	public static void lid(String[] args) {
		System.out.println("+--------+");
	}
	
	public static void stop(String[] args) {
		System.out.println("|  STOP  |");
	}
	
	public static void egg(String[] args) {
		openDown()
		openUp()
	}
	}

}
